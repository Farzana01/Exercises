package Maps;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashMap;
import java.util.Map;

public class MapsWithArrayList {

	public static void main(String[] args) {

		ArrayList<String[]> shelter = new ArrayList<String[]>();
		Arrays.asList("dogs", "cats","volunteer");
 		
		Map<String, String> myPets = new HashMap<String, String>();
		myPets.put("dogs", "Brady");
		myPets.put("cats", "Kitty");
		myPets.put("volunteer", "Peony");
		System.out.println(myPets.get("volunteer"));
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
