}
    // say 3 times
	public String say() {
		count ++;
		if (count == 3) {
		return "Fizz" + count;
	}
		public String say(int times) {
		String response = null;
		 for (int count = 1; count <= times; count++) {
			count ++;
			if (count == 3) {
			return "Fizz" + count;
}
